server:
	name=custom
	host=127.0.0.1
	port=8080
	error_page_400=custom/400.html
	error_page_404=custom/404.html
	error_page_500=custom/500.html

	route:
		uri=/
		path=www/srcs/
		index=index.html
		get=true
		post=true
		delete=false
		autoindex=true
		cgi_script=www/cgi/form.cgi.php
		cgi_bin=/usr/bin/php-cgi
		client_max_body_size=1K
		upload_dir=upload/
	;
;
