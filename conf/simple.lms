server:
	name=custom
	host=127.0.0.2
	port=2222
	error_page_400=custom/400.html
	error_page_404=custom/404.html
	error_page_500=custom/500.html

	route:
		uri=/custom
		path=www/custom
		index=custom.html
		get=true
		post=false
		delete=false
		autoindex=false
		cgi=www/cgi/env.cgi.php
		cgi_php=/usr/bin/php-cgi82
		cgi_python=/usr/bin/python
		client_max_body_size=1K
		upload_dir=www/custom/upload
	;
;
