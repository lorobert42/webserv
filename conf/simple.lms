server:
	name=custom
	host=127.0.0.1
	port=8080
	error_page_400=custom/400.html
	error_page_404=custom/404.html
	error_page_500=custom/500.html

	route:
		uri=/
		path=www/srcs
		index=index.html
		get=true
		post=false
		delete=false
		autoindex=false
		cgi_script=www/cgi/env.cgi.php
		cgi_bin=/usr/bin/php-cgi82
		client_max_body_size=1K
		upload_dir=www/custom/upload
	;
;
